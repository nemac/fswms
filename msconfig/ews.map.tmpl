        END  		
        METADATA
            "wms_title"                  "Firescars 2000-2013"
            "wms_abstract"               "Firescars 2000-2013"
            "wcs_label"                  "firescars WCS"
            "wcs_rangeset_name"          "firescars WCS"
            "wcs_rangeset_label"         "firescars WCS"
            "gml_include_items"          "class"		
        END  # end METADATA
